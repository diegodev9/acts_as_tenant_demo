<h1>Projects</h1>

<table class="table table-responsive">
  <thead>
    <tr>
      <th scope="col">Account</th>
      <th scope="col">Name</th>
      <!--<th colspan="3"></th>-->
    </tr>
  </thead>

  <tbody>
    <% @projects.each do |project| %>
      <tr>
        <td><%= project.account_id %></td>
        <td><%= project.name %></td>
        <!--<td><%#= link_to 'Show', project, class: 'btn btn-sm btn-primary' %></td>
        <td><%#= link_to 'Edit', edit_project_path(project), class: 'btn btn-sm btn-primary' %></td>
        <td><%#= link_to 'Destroy', project, method: :delete, data: { confirm: 'Are you sure?' }, class: 'btn btn-sm btn-danger' %></td>-->
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Project', new_project_path, class: 'btn btn-success' %>
